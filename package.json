{
  "name": "myagents",
  "description": "MyAgents - Desktop Claude Code for everyone. AI-powered productivity, not just for developers.",
  "type": "module",
  "version": "0.1.10",
  "license": "Apache-2.0",
  "packageManager": "bun@1.3.2",
  "author": {
    "name": "MyAgents Team",
    "email": "myagents.io@gmail.com"
  },
  "homepage": "https://myagents.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/hAcKlyc/MyAgents.git"
  },
  "bugs": {
    "url": "https://github.com/hAcKlyc/MyAgents/issues"
  },
  "keywords": [
    "claude",
    "ai",
    "agent",
    "desktop",
    "tauri",
    "productivity",
    "anthropic"
  ],
  "scripts": {
    "dev": "bun run dev:single",
    "dev:web": "vite",
    "dev:single": "bun run build:web --watch & bun run server",
    "build:web": "vite build",
    "server": "bun src/server/index.ts",
    "start": "bun run build:web && bun run server",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache --cache-location .eslintcache --max-warnings=0",
    "test": "bun test",
    "format": "prettier --experimental-cli --write .",
    "format:check": "prettier --experimental-cli --check --log-level warn .",
    "clean": "rm -rf node_modules dist .turbo .eslintcache *.tsbuildinfo src-tauri/target",
    "version": "node scripts/sync-version.js && git add src-tauri/tauri.conf.json src-tauri/Cargo.toml"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.33",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.3.4",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "adm-zip": "^0.5.16",
    "js-yaml": "^4.1.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.554.0",
    "mermaid": "^11.12.2",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.0",
    "react-arborist": "^3.4.3",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-katex": "^7.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/cli": "^2.9.6",
    "@types/adm-zip": "^0.5.7",
    "@types/bun": "^1.3.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.1.12"
  }
}
